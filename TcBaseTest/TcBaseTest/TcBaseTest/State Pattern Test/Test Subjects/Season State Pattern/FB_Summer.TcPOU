    <Method Name="OnEntry" Id="{b10c394c-e0f2-4325-b27c-db60abf04f67}">
      <Declaration><![CDATA[METHOD OnEntry
]]></Declaration>
      <Implementation>
        <ST><![CDATA[SUPER^.OnEntry();
