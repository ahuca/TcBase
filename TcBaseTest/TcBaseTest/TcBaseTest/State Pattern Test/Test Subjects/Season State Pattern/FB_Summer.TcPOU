    <Method Name="NextSeason" Id="{8867b54c-bdc6-4d81-b544-42189fdde5ac}">
      <Declaration><![CDATA[METHOD  NextSeason
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
